{
	//"workbench.colorTheme": "City Lights",
	//"workbench.iconTheme": "city-lights-icons-vsc-light",
	"workbench.colorTheme": "GNOME dark",
	"workbench.iconTheme": "material-icon-theme",
	"workbench.productIconTheme": "fluent-icons",
	"window.zoomLevel": 0,
	"terminal.integrated.shell.windows": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
	"settingsSync.keybindingsPerPlatform": false,
	"settingsSync.ignoredExtensions": [],
	"diffEditor.ignoreTrimWhitespace": false,
	"git.autofetch": true,
	"gitlens.defaultDateLocale": null,
	"material-icon-theme.hidesExplorerArrows": false,
	"material-icon-theme.activeIconPack": "vue",
	"material-icon-theme.saturation": 0.8,
	"material-icon-theme.folders.color": "#dcb67a",
	"vsicons.dontShowNewVersionMessage": true,
	"redhat.telemetry.enabled": true,
	"[css]": {
		"editor.defaultFormatter": "vscode.css-language-features"
	},
	"go.formatTool": "goimports",
	"go.useLanguageServer": true,
	"csharp.semanticHighlighting.enabled": true,
	"omnisharp.enableImportCompletion": true,
	"omnisharp.enableRoslynAnalyzers": true,
	"omnisharp.organizeImportsOnFormat": true,
	"platformio-ide.activateOnlyOnPlatformIOProject": true
}